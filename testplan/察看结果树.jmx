        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="提取access_token" enabled="true">
          <stringProp name="BeanShellSampler.query">import com.alibaba.fastjson.JSONObject;




String tokenStr =prev.getResponseDataAsString();  // 获取上一步请求的整个响应数据

log.info(&quot;tokenStr = &quot; + vars.get(&quot;tokenStr&quot;));




JSONObject jo = JSONObject.parseObject(tokenStr); // 浏览器返回的字符串解析为正常的json格式，jmeter没有提供对于Json数据的响应处理

String access_token1 = jo.get(&quot;access_token&quot;);

vars.put(&quot;access_token2&quot;,access_token1);</stringProp>
          <stringProp name="BeanShellSampler.filename"></stringProp>
